{
  "buildCommand": "cd sistema-gestion && npm install && npm run build",
  "outputDirectory": "sistema-gestion/dist",
  "installCommand": "cd sistema-gestion && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://sistemagestion-production.up.railway.app/api/$1"
    }
  ]
}
